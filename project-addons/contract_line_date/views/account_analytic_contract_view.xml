<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="account_analytic_contract_view_form_add_dates" model="ir.ui.view">
            <field name="name">Account Analytic Contract Form View</field>
            <field name="model">account.analytic.contract</field>
            <field name="inherit_id" ref="contract.account_analytic_contract_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='recurring_invoice_line_ids']//field[@name='analytic_tag_ids']" position="after">
                    <field name="date_from"/>
                    <field name="date_to"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
